    "react-day-picker": "^7.1.1",
    "react-dom": "15.4.2",
    "react-month-picker": "^1.3.5",
    "react-object-list": "^0.0.43",
    "react-select": "^2.0.0-beta.6",
    "regenerator-runtime": "^0.12.1"
  },
